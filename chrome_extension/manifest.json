{
	"manifest_version": 3,
	"name": "phurhardScreenRecorder",
	"description": "Records the screen",
	"icons": {
		"128": "./images/icon.png"
	},
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "./images/icon.png"
		}
	},
	"host_permissions": [
		"http://*/",
		"https://*/*"
	],
	"permissions": [
		"scripting",
		"tabs",
		"activeTab"
	],
	"version": "1.0.0"
}
